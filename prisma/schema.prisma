generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  discordId String   @unique
  username  String
  avatar    String
  email     String?  @unique
  createdAt DateTime @default(now())
}
